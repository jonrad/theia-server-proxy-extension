{
  "private": true,
  "scripts": {
    "preinstall": "node-gyp install",
    "bootstrap": "lerna bootstrap",
    "prepare": "lerna run prepare",
    "rebuild:browser": "theia rebuild --app-target=browser",
    "start:browser": "yarn rebuild:browser && yarn --cwd packages/browser-app start --hostname 0.0.0.0",
    "watch": "lerna run --parallel watch",
    "build:docker": "docker build . -t jonrad/theia-datascience",
    "publish:docker": "yarn build:docker && docker push jonrad/theia-datascience"
  },
  "devDependencies": {
    "lerna": "2.4.0"
  },
  "resolutions": {
    "**/**/nsfw": "1.2.9"
  },
  "workspaces": [
    "packages/*"
  ],
  "homepage": "https://github.com/jonrad/theia-server-proxy-extension",
  "version": "0.0.1"
}
