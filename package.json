{
  "private": true,
  "scripts": {
    "preinstall": "node-gyp install",
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build",
    "rebuild:browser": "theia rebuild:browser",
    "start:browser": "yarn rebuild:browser && yarn --cwd browser-app start",
    "watch": "lerna run --parallel watch",
    "build:docker": "docker build . -t jonrad/theia-datascience",
    "publish:docker": "yarn build:docker && docker push jonrad/theia-datascience"
  },
  "devDependencies": {
    "lerna": "2.4.0"
  },
  "resolutions": {
    "**/**/nsfw": "1.2.9"
  },
  "workspaces": [
    "theia-server-proxy-iframe-extension",
    "theia-server-proxy-extension",
    "theia-server-proxy-list-extension",
    "theia-jupyter-extension",
    "theia-rstudio-extension",
    "browser-app"
  ],
  "version": "0.0.1"
}
